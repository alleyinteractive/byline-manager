{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "byline-manager/byline",
	"version": "0.1.0",
	"title": "Byline",
	"category": "text",
	"icon": "users",
	"description": "Displays bylines.",
	"textdomain": "byline",
	"editorScript": "file:index.ts",
	"editorStyle": "file:index.css",
	"style": [
		"file:style-index.css"
	],
	"render": "file:render.php",
  "attributes": {
    "author": {
      "type": "string",
      "default": ""
    },
    "link": {
      "type": "string"
    }
  }
}
